msgid "Continue with <b>Google</b>"
msgstr "Continuer avec <b>Google</b>"

#: providers/google/google.php:102
msgid "Link account with <b>Google</b>"
msgstr "Lier le compte avec <b>Google</b>"

#: providers/google/google.php:103
